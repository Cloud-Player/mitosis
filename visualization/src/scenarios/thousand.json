{
  "benchmark": {
    "default": {
      "LAST_SEEN_TIMEOUT": {
        "from": 0,
        "to": 60,
        "step": 5
      }
    }
  },
  "configuration": {
    "sub-ticks": 4,
    "newbie": {
      "DEFAULT_SIGNAL_ADDRESS": "mitosis/v1/signal/wss"
    }
  },
  "instructions": [
    {
      "tick": 0,
      "type": "set-clock-speed",
      "configuration": {
        "speed": 1
      }
    },
    {
      "tick": 1,
      "type": "add-peer",
      "configuration": {
        "address": "mitosis/v1/signal/wss",
        "roles": [
          "signal"
        ]
      }
    },
    {
      "tick": 1,
      "type": "generate-peers",
      "configuration": {
        "count": 100
      }
    },
    {
      "tick": 2500,
      "type": "finish-scenario"
    }
  ]
}
